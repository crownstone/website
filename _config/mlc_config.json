{
    "aliveStatusCodes": [200, 206, 0, 999],
    "projectBaseUrl": "https://crownstone.rocks",
    "replacementPatterns": [
        {
            "pattern": "^{{site.url}}/",
            "replacement": "{{BASEURL}}/"
        },
        {
            "pattern": "^%7B%7Bsite.url%7D%7D",
            "replacement": "{{BASEURL}}/"
        },
        {
            "pattern": "^{{ site.url }}/",
            "replacement": "{{BASEURL}}/"
        },
        {
            "pattern": "^/",
            "replacement": "{{BASEURL}}/"
        }
    ],
    "ignorePatterns": [
        {
            "pattern": "^#.*$"
        },
        {
            "pattern": "https://linkedin.com/company/*"
        },
        {
            "pattern": "https://www.crunchbase.com/*"
        },
        {
            "pattern": "https://www.kickstarter.com/*"
        },
        {
            "pattern": "https://trends.google.com/*"
        }

    ]
}
